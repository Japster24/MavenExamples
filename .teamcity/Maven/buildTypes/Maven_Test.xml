<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE build-type SYSTEM "../../../project-config.dtd">

<build-type paused="true">
  <name>Test</name>
  <description />
  <settings>
    <options>
      <option name="cleanBuild" value="true" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="mavenSnapshotDependencyTrigger" />
    </disabled-settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_14" name="" type="Duplicator">
        <parameters>
          <param name="duplicates.runner.bound" value="10" />
          <param name="duplicates.runner.discard" value="0" />
          <param name="duplicates.runner.literal" value="true" />
          <param name="duplicates.runner.method" value="true" />
          <param name="duplicates.runner.type" value="true" />
          <param name="duplicates.runner.visibility" value="true" />
          <param name="iprInfoRunParam" value="&lt;iprInfo path-to-ipr=&quot;mvnexbook-examples-1.0/pom.xml&quot; should-load-iml=&quot;false&quot; /&gt;" />
          <param name="jvmArgs" value="-Xmx512m -XX:MaxPermSize=150m" />
          <param name="target.jdk.home" value="%env.JDK_16%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Maven_HttpsGithubComJapster24MavenExamplesGitRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="mavenSnapshotDependencyTrigger" type="mavenSnapshotDependencyTrigger">
        <parameters>
          <param name="skipIfRunning" value="true" />
        </parameters>
      </build-trigger>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="groupCheckinsByCommitter" value="true" />
          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
