<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE build-type SYSTEM "../../../project-config.dtd">

<build-type>
  <name>Production</name>
  <description />
  <settings>
    <options>
      <option name="cleanBuild" value="true" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters>
      <param name="areyousure" value="" spec="checkbox checkedValue='true' description='Check if you want to deploy a build' uncheckedValue='false' label='To deploy or not to deploy' display='prompt'" />
      <param name="mavenVer" value="" spec="select label_1='empty' data_5='versions:set -DnewVersion=%ver%' data_1='' display='prompt' label='Release Type' data_3='versions:set -DnewVersion=%ver%-SNAPSHOT' label_3='snapshot' label_5='release'" />
      <param name="ver" value="0.1" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_19" name="Verify deployment" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x86" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_script_code"><![CDATA[If(%areyousure% -eq true) {return 0}
Else {return 1}]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.deployerUsername" value="admin" />
          <param name="secure:org.jfrog.artifactory.selectedDeployableServer.deployerPassword" value="zxx192f478978efc1c5775d03cbe80d301b" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_18" name="" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x86" />
          <param name="jetbrains_powershell_execution" value="STDIN" />
          <param name="jetbrains_powershell_script_code" value="echo &quot;DEPLOY THIS BEAST TO RELEASE&quot;" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Maven_HttpsGithubComJapster24MavenExamplesGitRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="Maven_Deploy">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

